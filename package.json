{
  "name": "@devwareng/vanilla-ts",
  "version": "1.1.12",
  "description": "Framework-less TypeScript hooks for SPA development.",
  "author": "Waren Arapoc Gador",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup index.mts --minify --format esm,cjs --dts --outDir dist"
  },
  "keywords": [
    "typescript",
    "vanilla",
    "hooks",
    "spa",
    "frameworkless",
    "utilities",
    "frontend"
  ],
  "dependencies": {
    "dompurify": "^3.2.6",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.20",
    "tsup": "^8.5.0",
    "typescript": "^5.3.0"
  }
}
